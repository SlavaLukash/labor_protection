        <Name>trauma</Name>
        <NameInSource>labor</NameInSource>
        <Parameters isList="true" count="0" />
        <SqlCommand>SELECT trauma.datetrauma, enterprise.name AS enterprise_name,
	enterprise.address AS address, enterprise.okved,
	traumakind.name AS traumakind_name, traumacause.name AS traumacause_name,
	subdivision.name AS subdivision_name,
	profession.name AS profession_name,
	marriagekind.name AS marriagekind_name,
	employee.lastname,
	employee.firstname,
	employee.middlename,
	employee.sex,
	employee.bithday AS birthday,
	employee.date_first_medical,
	employee.date_instruction	
	FROM trauma INNER JOIN
	employee ON trauma.employee = employee.id
  INNER JOIN
  subdivision ON employee.subdivision =
    subdivision.id INNER JOIN
  /*SECURITY*/enterprise enterprise ON subdivision.enterprise =
    enterprise.id INNER JOIN
  traumakind ON trauma.traumakind = traumakind.id
  INNER JOIN
  traumacause ON trauma.traumacause = traumacause.id
  join profession on employee.profession = profession.id
  join marriagekind on employee.marriagekind = marriagekind.id
WHERE trauma.id = {VarId}</SqlCommand>
      </trauma>
